from audioop import reverse
from random import*
a=input("Введите первую строку: ")
b=input("Введите вторую строку: ")
a_list=list(a)
b_list=list(b)
print("1 список")
print(a_list)
print("2 список")
print(b_list)
print("Выберети один из предложеных вариантов")
while True:
    print("1 - Дабавить букву в список")
    print("2 - Удалить элемент по его индексу")
    print("3 - Получение индекса первого вхождения элемента")
    print("4 - Объединение двух списков")
    print("5 - Сортировка списка по возрастанию")
    print("6 - Сортировка списка по убыванию")
    print("7 - Создание списка из диапазона чисел:")
    print("8 - ")
    print("9 - ")
    print("10- ")
    valik=int(input())
    if valik==1:
        V=input("В какой из списков добавить символ")
        if V==1:
            w=input("Введите букву: ")
            a_append=a_list.append(w)
            print(a_append)
        elif V==2:
            w=input("Введите букву: ")
            b_append=b_list.append(w)
            print(b_append)
        else:
            print("Error!")
    elif valik==2:
        V=input("С какого списка удалить символ")
        if V==1:
            ind=input("Введите индекс символа")
            a_pop=a_list.pop(ind)
            print(a_pop)
        elif V==2:
            ind=input("Введите индекс символа")
            b_pop=b_list.pop(ind)
            print(b_pop)
        else:
            print("Error!")
    elif valik==3:
        V=input("Получение индекса первого вхождения элемента")
        if V==1:
            a=input("Введите элемент:")
            a_il=a_list.index(a)
            print(a_il)
        elif V==2:
            a=input("Введите элемент:")
            b_il=b_list.index(a)
            print(b_il)
        else:
            print("Error!")
    elif valik==4:
        M=a_list + b_list
        print(M)
    elif valik==5:
        V=input("Какой из списков отсортировать")
        if V==1:
            a_sort=a_list.sort()
            print(a_sort)
        elif V==2:
            b_sort=b_list.sort()
            print(b_sort)
        else:
            print("Error!")
    elif valik==6:
        V=input("Какой из списков отсортировать")
        if V==1:
            sort_a=a_list.sort(reverse=True)
            print(sort_a)
        elif V==2:
            sort_b=b_list.sort(reverse=True)
            print(sort_b)
        else:
            print("Error!")
    elif valik==7:
        try:
            g=int(input("Введи до скольки будет список"))
        except:
            print("Error")
        c_list = list(range(g))
        print(c_list)
    elif valik==8:
        V=input("Какой из списков скопировать")
        if V==1:
            a_copy=a_list.copy()
            print(a_copy)
        elif V==2:
            b_copy=a_list.copy()
            print(b_copy)
        else:
            print("Error!")
    elif valik==9:
    elif valik==10:

        V=input("Какой из списков отсортировать")
